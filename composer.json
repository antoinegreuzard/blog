{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^3.2",
        "dama/doctrine-test-bundle": "^8.1",
        "doctrine/dbal": "^4.2.4",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-fixtures-bundle": "^4.1.0",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.0",
        "lexik/jwt-authentication-bundle": "^3.0",
        "nelmio/cors-bundle": "^2.4",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpdoc-parser": "^2.1.0",
        "phpstan/phpstan": "^1.11",
        "phpstan/phpstan-doctrine": "^1.4",
        "phpstan/phpstan-symfony": "^1.4",
        "symfony/asset": "7.3.0",
        "symfony/asset-mapper": "7.3.0",
        "symfony/browser-kit": "7.3.0",
        "symfony/console": "7.3.1",
        "symfony/css-selector": "7.3.0",
        "symfony/debug-bundle": "7.3.0",
        "symfony/doctrine-bridge": "7.3.0",
        "symfony/doctrine-messenger": "7.3.0",
        "symfony/dotenv": "7.3.0",
        "symfony/expression-language": "7.3.0",
        "symfony/flex": "^2",
        "symfony/form": "7.3.0",
        "symfony/framework-bundle": "7.2.5",
        "symfony/http-client": "7.3.0",
        "symfony/intl": "7.3.1",
        "symfony/mailer": "7.3.0",
        "symfony/mime": "7.3.0",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.3.0",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/process": "7.3.0",
        "symfony/property-access": "7.3.0",
        "symfony/property-info": "7.3.1",
        "symfony/runtime": "7.3.0",
        "symfony/security-bundle": "7.3.1",
        "symfony/serializer": "7.3.1",
        "symfony/stimulus-bundle": "^2.14",
        "symfony/stopwatch": "7.3.0",
        "symfony/string": "7.3.0",
        "symfony/translation": "7.3.0",
        "symfony/twig-bundle": "7.3.1",
        "symfony/ux-turbo": "^2.14",
        "symfony/validator": "7.3.1",
        "symfony/web-link": "7.3.0",
        "symfony/web-profiler-bundle": "7.3.0",
        "symfony/yaml": "7.3.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/filesystem": "^7.2",
        "symfony/dependency-injection": "^7.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpcs": "phpcs",
        "phpcbf": "phpcbf",
        "php-cs-fixer": "php-cs-fixer fix",
        "phpstan": "phpstan analyse"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2.2",
        "escapestudios/symfony2-coding-standard": "^3.13",
        "squizlabs/php_codesniffer": "^3.10",
        "symfony/maker-bundle": "^1.60"
    }
}
